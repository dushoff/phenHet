	journal = {Prev Med},
	keywords = {Adolescent Adult Cross-Sectional Studies Female Humans Male Opioid-Related Disorders/epidemiology/*psychology Risk-Taking Sex Distribution Sexual Behavior/*psychology/*statistics & numerical data Sexually Transmitted Diseases/*epidemiology/*psychology Substance Abuse, Intravenous/epidemiology/*psychology United States/epidemiology Young Adult Chlamydia/gonorrhea Injection drug use Nationally representative data Opioids STDs Sexual risk behavior Syphilis},
