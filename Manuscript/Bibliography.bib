	keywords = {Pregnancy Female United States/epidemiology Infant, Newborn Humans Retrospective Studies *Pregnancy Complications, Infectious/diagnosis/epidemiology *Syphilis/diagnosis/epidemiology *Premature Birth/epidemiology/etiology Risk Factors disparities preterm birth screening sexual infections sexually transmitted sociodemographic factors},
	note = {Gulersen, Moti Lenchner, Erez Eliner, Yael Grunebaum, Amos Johnson, Lisa Chervenak, Frank A Bornstein, Eran eng 2023/04/08 Am J Obstet Gynecol MFM. 2023 Jun;5(6):100957. doi: 10.1016/j.ajogmf.2023.100957. Epub 2023 Apr 6.},
	number = {6},
