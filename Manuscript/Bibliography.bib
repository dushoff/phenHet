	note = {Cai, R Tan, J G Chen, L Richardus, J H de Vlas, S J eng England 2013/10/15 Trop Med Int Health. 2013 Dec;18(12):1531-8. doi: 10.1111/tmi.12204. Epub 2013 Oct 7.},
	number = {12},
	pages = {1531-8},
	title = {Prevalence and risk factors of syphilis infection among female sex workers in Shenzhen, China: an observational study (2009-2012)},
