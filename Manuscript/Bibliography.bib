	note = {Warner, L Rochat, R W Fichtner, R R Stoll, B J Nathan, L Toomey, K E eng 2001/03/10 Sex Transm Dis. 2001 Feb;28(2):92-8. doi: 10.1097/00007435-200102000-00006.},
	number = {2},
