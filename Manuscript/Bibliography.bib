	journal = {BMC Infect Dis},
	keywords = {AIDS-Related Opportunistic Infections/epidemiology/etiology Adult Gonorrhea/epidemiology HIV Infections/epidemiology/microbiology HIV Seropositivity/*epidemiology HIV Serosorting Homosexuality, Male Humans Male Prospective Studies Risk Factors Sexual and Gender Minorities Spain/epidemiology Syphilis/*epidemiology/*etiology Treponema pallidum/pathogenicity Condomless anal sex Hiv Men who have sex with men Syphilis},
