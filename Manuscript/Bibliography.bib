	keywords = {Adult Birth Certificates Case-Control Studies Female Florida/epidemiology Humans Infant, Newborn Multivariate Analysis Odds Ratio Pregnancy Pregnancy Complications, Infectious/*epidemiology Regression Analysis Risk Factors Socioeconomic Factors Syphilis/*epidemiology Syphilis, Congenital/*epidemiology},
	note = {Desenclos, J C Scaggs, M Wroten, J E eng 1992/09/15 Am J Epidemiol. 1992 Sep 15;136(6):657-61. doi: 10.1093/oxfordjournals.aje.a116545.},
	number = {6},
	pages = {657-61},
	title = {Characteristics of mothers of live infants with congenital syphilis in Florida, 1987-1989},
