	note = {Southwick, K L Guidry, H M Weldon, M M Mert, K J Berman, S M Levine, W C eng 1999/04/07 Am J Public Health. 1999 Apr;89(4):557-60. doi: 10.2105/ajph.89.4.557.},
	number = {4},
	pages = {557-60},
	title = {An epidemic of congenital syphilis in Jefferson County, Texas, 1994-1995: inadequate prenatal syphilis testing after an outbreak in adults},
