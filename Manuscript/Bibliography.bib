	note = {Aung, Ei T Fairley, Christopher K Ong, Jason J Chen, Marcus Y Phillips, Tiffany R Tran, Julien Samra, Ranjit Chow, Eric P F eng 2023/02/09 Open Forum Infect Dis. 2023 Jan 12;10(2):ofad017. doi: 10.1093/ofid/ofad017. eCollection 2023 Feb.},
	number = {2},
	pages = {ofad017},
